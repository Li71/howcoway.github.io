@using DevStudio.Securitys;
@using goshopping.Models;
@{
    if (UserAccount.UserCode == 0)
    { ViewBag.Title = "廠商訂單查詢 / 未結訂單查詢"; }
    else
    { ViewBag.Title = "廠商訂單查詢 / 歷史訂單查詢"; }
    Layout = "~/Views/Shared/_LayoutBackend.cshtml";
}

<div style="width:100%; margin:0 auto" class="tablecontainer">
    <table id="DatatableList" class="table table-bordered  display dt-responsive nowrap">
        <thead>
            <tr>
                <th>訂單編號</th>
                <th>訂單日期</th>
                <th>訂單狀態</th>
                <th>會員編號</th>
                <th>商品代號</th>
                <th>商品名稱</th>
                <th>商品規格</th>
                <th>數量</th>
                <th>單價</th>
                <th>小計</th>
                <th>備註</th>
            </tr>
        </thead>
    </table>
</div>

<script src="@Url.Content("~/Scripts/apps/vendor/vendor_order.js")"></script>